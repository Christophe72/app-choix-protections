// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model CircuitType {
  id             Int      @id @default(autoincrement())
  nom            String
  section        String
  longueurMax    Int
  disjoncteur    Int
  typeCourbe     String
  differentiel   String
  remarque       String?
}

model NormeRGIE {
  id          Int    @id @default(autoincrement())
  article     String
  description String
}
